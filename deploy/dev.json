{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "labels": {
      "team": "arbeidsgiver"
    },
    "name": "helse-spion",
    "namespace": "default"
  },
  "spec": {
    "cpuThresholdPercentage": 90,
    "image": "repo.adeo.no:5443/helse-spion:RELEASE_VERSION",
    "istio": {
      "enabled": false
    },
    "liveness": {
      "failureThreshold": 3,
      "initialDelay": 60,
      "path": "/internal/isAlive",
      "periodSeconds": 10,
      "timeout": 1
    },
    "port": 8080,
    "readiness": {
      "failureThreshold": 3,
      "initialDelay": 60,
      "path": "/internal/isReady",
      "periodSeconds": 10,
      "timeout": 1
    },
    "replicas": {
      "max": 1,
      "min": 1
    },
    "resources": {
      "limits": {
        "cpu": "100m",
        "memory": "128Mi",
        "requests": {
          "cpu": "50m",
          "memory": "32Mi"
        }
      }
    },
    "ingresses": "https://helse-spion.nais.preprod.local",
    "vault": {
      "enabled": false
    }
  }
}
